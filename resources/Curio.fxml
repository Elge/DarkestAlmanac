<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml"
      xmlns="http://javafx.com/javafx"
      fx:controller="de.sgoral.darkestalmanac.ui.controllers.CurioController"
      prefWidth="400">
    <Label fx:id="locations"/>
    <TableView fx:id="experimentsTable">
        <columns>
            <TableColumn fx:id="consumableColumn" text="Consumable"/>
            <TableColumn fx:id="effectsColumn" text="Effects"/>
        </columns>
    </TableView>
</VBox>
